<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CBT_Suite">
	<test thread-count="5" name="chromeTestResults">
		<parameter name="browserName" value="chrome"></parameter>
		<classes>
			<class name="testng.CrossBrowserTesting" />
			<class name="testng.ClgWeekLive" />			
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="edgeTestResults">
		<parameter name="browserName" value="edge"></parameter>
		<classes>
			<class name="testng.CrossBrowserTesting" />
			<class name="testng.ClgWeekLive" />		
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="firefoxTestResults">
		<parameter name="browserName" value="firefox"></parameter>
		<classes>
			<class name="testng.CrossBrowserTesting" />
			<class name="testng.ClgWeekLive" />		
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
